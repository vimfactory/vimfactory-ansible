---
user: {{ appuser }}
group: {{ appgroup }}
pid: vimfactory-web.pid
port: 9292
timeout: 30
wait: 30
log: logs/thin.log
max_conns: 1024
require: []
environment: {{ env }}
max_persistent_conns: 512
servers: 1
threaded: true
no-epoll: true
daemonize: true
rackup: config.ru
chdir: {{ app_root }}/{{ web_repository_name }}
